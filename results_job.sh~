#!/bin/bash -l
#SBATCH -n 1
#SBATCH --job-name=s_o_class
#SBATCH --output=logs/s_o_class.%j
#SBATCH --time=2-00:00
#SBATCH --mem=32GB
module load libs/openmpi
module load devtools/anaconda
conda deactivate
conda activate variational_orthogonal_classifiers
mpirun -n 1 python experiments.py
echo 'finished'
